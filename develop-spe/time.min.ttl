:after
    rdf:type      owl:ObjectProperty ;
    rdfs:domain   :TemporalEntity ;
    rdfs:range    :TemporalEntity ;
    owl:inverseOf :before ;
.

:before
    rdf:type      owl:ObjectProperty ;
    rdf:type      owl:TransitiveProperty ;
    rdfs:domain   :TemporalEntity ;
    rdfs:range    :TemporalEntity ;
    owl:inverseOf :after ;
.

:day
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :GeneralDateTimeDescription ;
.

:dayOfWeek
    rdf:type    owl:ObjectProperty ;
    rdfs:domain :GeneralDateTimeDescription ;
    rdfs:range  :DayOfWeek ;
.

:dayOfYear
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :GeneralDateTimeDescription ;
    rdfs:range  xsd:nonNegativeInteger ;
.

:days
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :GeneralDurationDescription ;
    rdfs:range  xsd:decimal ;
.

:generalDay
    rdf:type             rdfs:Datatype ;
    owl:onDatatype       xsd:string ;
    owl:withRestrictions ( [ xsd:pattern "---(0[1-9]|[1-9][0-9])(Z|(\\+|-)((0[0-9]|1[0-3]):[0-5][0-9]|14:00))?" ; ] ) ;
.

:generalMonth
    rdf:type             rdfs:Datatype ;
    owl:onDatatype       xsd:string ;
    owl:withRestrictions ( [ xsd:pattern "--(0[1-9]|1[0-9]|20)(Z|(\\+|-)((0[0-9]|1[0-3]):[0-5][0-9]|14:00))?" ; ] ) ;
.

:generalYear
    rdf:type             rdfs:Datatype ;
    owl:onDatatype       xsd:string ;
    owl:withRestrictions ( [ xsd:pattern
                                     "-?([1-9][0-9]{3,}|0[0-9]{3})(Z|(\\+|-)((0[0-9]|1[0-3]):[0-5][0-9]|14:00))?" ; ] ) ;
.

:hasBeginning
    rdf:type           owl:ObjectProperty ;
    rdfs:domain        :TemporalEntity ;
    rdfs:range         :Instant ;
    rdfs:subPropertyOf :hasTime ;
.

:hasDateTimeDescription
    rdf:type    owl:ObjectProperty ;
    rdfs:domain :DateTimeInterval ;
    rdfs:range  :GeneralDateTimeDescription ;
.

:hasDuration
    rdf:type           owl:ObjectProperty ;
    rdfs:range         :Duration ;
    rdfs:subPropertyOf :hasTemporalDuration ;
.

:hasDurationDescription
    rdf:type           owl:ObjectProperty ;
    rdfs:range         :GeneralDurationDescription ;
    rdfs:subPropertyOf :hasTemporalDuration ;
.

:hasEnd
    rdf:type           owl:ObjectProperty ;
    rdfs:domain        :TemporalEntity ;
    rdfs:range         :Instant ;
    rdfs:subPropertyOf :hasTime ;
.

:hasTRS
    rdf:type    owl:FunctionalProperty ;
    rdf:type    owl:ObjectProperty ;
    rdfs:domain [ rdf:type    owl:Class ;
                  owl:unionOf ( :TemporalPosition
                                :GeneralDurationDescription ) ; ] ;
    rdfs:range  :TRS ;
.

:hasTemporalDuration
    rdf:type    owl:ObjectProperty ;
    rdfs:domain :TemporalEntity ;
    rdfs:range  :TemporalDuration ;
.

:hasTime
    rdf:type   owl:ObjectProperty ;
    rdfs:range :TemporalEntity ;
.

:hasXSDDuration
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :TemporalEntity ;
    rdfs:range  xsd:duration ;
.

:hour
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :GeneralDateTimeDescription ;
    rdfs:range  xsd:nonNegativeInteger ;
.

:hours
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :GeneralDurationDescription ;
    rdfs:range  xsd:decimal ;
.

:inDateTime
    rdf:type           owl:ObjectProperty ;
    rdfs:domain        :Instant ;
    rdfs:range         :GeneralDateTimeDescription ;
    rdfs:subPropertyOf :inTemporalPosition ;
.

:inTemporalPosition
    rdf:type    owl:ObjectProperty ;
    rdfs:domain :Instant ;
    rdfs:range  :TemporalPosition ;
.

:inTimePosition
    rdf:type           owl:ObjectProperty ;
    rdfs:domain        :Instant ;
    rdfs:range         :TimePosition ;
    rdfs:subPropertyOf :inTemporalPosition ;
.

:inXSDDate
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :Instant ;
    rdfs:range  xsd:date ;
.

#:inXSDDateTime
#    rdf:type       owl:DatatypeProperty ;
#    rdf:type       owl:DeprecatedProperty ;
#    rdfs:domain    :Instant ;
#    rdfs:range     xsd:dateTime ;
#    owl:deprecated "true"^^xsd:boolean ;
#.

:inXSDDateTimeStamp
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :Instant ;
    rdfs:range  xsd:dateTimeStamp ;
.

:inXSDgYear
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :Instant ;
    rdfs:range  xsd:gYear ;
.

:inXSDgYearMonth
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :Instant ;
    rdfs:range  xsd:gYearMonth ;
.

:inside
    rdf:type    owl:ObjectProperty ;
    rdfs:domain :Interval ;
    rdfs:range  :Instant ;
.

:intervalAfter
    rdf:type           owl:ObjectProperty ;
    rdfs:domain        :ProperInterval ;
    rdfs:range         :ProperInterval ;
    rdfs:subPropertyOf :after ;
    rdfs:subPropertyOf :intervalDisjoint ;
    owl:inverseOf      :intervalBefore ;
.

:intervalBefore
    rdf:type           owl:ObjectProperty ;
    rdfs:domain        :ProperInterval ;
    rdfs:range         :ProperInterval ;
    rdfs:subPropertyOf :before ;
    rdfs:subPropertyOf :intervalDisjoint ;
    owl:inverseOf      :intervalAfter ;
.

:intervalContains
    rdf:type      owl:ObjectProperty ;
    rdfs:domain   :ProperInterval ;
    rdfs:range    :ProperInterval ;
    owl:inverseOf :intervalDuring ;
.

:intervalDisjoint
    rdf:type    owl:ObjectProperty ;
    rdfs:domain :ProperInterval ;
    rdfs:range  :ProperInterval ;
.

:intervalDuring
    rdf:type           owl:ObjectProperty ;
    rdfs:domain        :ProperInterval ;
    rdfs:range         :ProperInterval ;
    rdfs:subPropertyOf :intervalIn ;
    owl:inverseOf      :intervalContains ;
.

:intervalEquals
    rdf:type                 owl:ObjectProperty ;
    rdfs:domain              :ProperInterval ;
    rdfs:range               :ProperInterval ;
    owl:propertyDisjointWith :intervalIn ;
.

:intervalFinishedBy
    rdf:type      owl:ObjectProperty ;
    rdfs:domain   :ProperInterval ;
    rdfs:range    :ProperInterval ;
    owl:inverseOf :intervalFinishes ;
.

:intervalFinishes
    rdf:type           owl:ObjectProperty ;
    rdfs:domain        :ProperInterval ;
    rdfs:range         :ProperInterval ;
    rdfs:subPropertyOf :intervalIn ;
    owl:inverseOf      :intervalFinishedBy ;
.

:intervalIn
    rdf:type                 owl:ObjectProperty ;
    rdfs:domain              :ProperInterval ;
    rdfs:range               :ProperInterval ;
    owl:propertyDisjointWith :intervalEquals ;
.

:intervalMeets
    rdf:type      owl:ObjectProperty ;
    rdfs:domain   :ProperInterval ;
    rdfs:range    :ProperInterval ;
    owl:inverseOf :intervalMetBy ;
.

:intervalMetBy
    rdf:type      owl:ObjectProperty ;
    rdfs:domain   :ProperInterval ;
    rdfs:range    :ProperInterval ;
    owl:inverseOf :intervalMeets ;
.

:intervalOverlappedBy
    rdf:type      owl:ObjectProperty ;
    rdfs:domain   :ProperInterval ;
    rdfs:range    :ProperInterval ;
    owl:inverseOf :intervalOverlaps ;
.

:intervalOverlaps
    rdf:type      owl:ObjectProperty ;
    rdfs:domain   :ProperInterval ;
    rdfs:range    :ProperInterval ;
    owl:inverseOf :intervalOverlappedBy ;
.

:intervalStartedBy
    rdf:type      owl:ObjectProperty ;
    rdfs:domain   :ProperInterval ;
    rdfs:range    :ProperInterval ;
    owl:inverseOf :intervalStarts ;
.

:intervalStarts
    rdf:type           owl:ObjectProperty ;
    rdfs:domain        :ProperInterval ;
    rdfs:range         :ProperInterval ;
    rdfs:subPropertyOf :intervalIn ;
    owl:inverseOf      :intervalStartedBy ;
.

:minute
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :GeneralDateTimeDescription ;
    rdfs:range  xsd:nonNegativeInteger ;
.

:minutes
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :GeneralDurationDescription ;
    rdfs:range  xsd:decimal ;
.

:month
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :GeneralDateTimeDescription ;
.

:monthOfYear
    rdf:type    owl:ObjectProperty ;
    rdfs:domain :GeneralDateTimeDescription ;
    rdfs:range  :MonthOfYear ;
.

:months
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :GeneralDurationDescription ;
    rdfs:range  xsd:decimal ;
.

:nominalPosition
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :TimePosition ;
    rdfs:range  xsd:string ;
.

:numericDuration
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :Duration ;
    rdfs:range  xsd:decimal ;
.

:numericPosition
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :TimePosition ;
    rdfs:range  xsd:decimal ;
.

:second
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :GeneralDateTimeDescription ;
    rdfs:range  xsd:decimal ;
.

:seconds
    rdf:type     owl:DatatypeProperty ;
    rdfs:domain  :GeneralDurationDescription ;
    rdfs:range   xsd:decimal ;
    rdfs:seeAlso <http://www.bipm.org/en/publications/si-brochure/second.html> ;
.

:timeZone
    rdf:type    owl:ObjectProperty ;
    rdfs:domain :GeneralDateTimeDescription ;
    rdfs:range  :TimeZone ;
.

:unitDay
    rdf:type :TemporalUnit ;
    :days    "1"^^xsd:decimal ;
    :hours   "0"^^xsd:decimal ;
    :minutes "0"^^xsd:decimal ;
    :months  "0"^^xsd:decimal ;
    :seconds "0"^^xsd:decimal ;
    :weeks   "0"^^xsd:decimal ;
    :years   "0"^^xsd:decimal ;
.

:unitHour
    rdf:type :TemporalUnit ;
    :days    "0"^^xsd:decimal ;
    :hours   "1"^^xsd:decimal ;
    :minutes "0"^^xsd:decimal ;
    :months  "0"^^xsd:decimal ;
    :seconds "0"^^xsd:decimal ;
    :weeks   "0"^^xsd:decimal ;
    :years   "0"^^xsd:decimal ;
.

:unitMinute
    rdf:type :TemporalUnit ;
    :days    "0"^^xsd:decimal ;
    :hours   "0"^^xsd:decimal ;
    :minutes "1"^^xsd:decimal ;
    :months  "0"^^xsd:decimal ;
    :seconds "0"^^xsd:decimal ;
    :weeks   "0"^^xsd:decimal ;
    :years   "0"^^xsd:decimal ;
.

:unitMonth
    rdf:type :TemporalUnit ;
    :days    "0"^^xsd:decimal ;
    :hours   "0"^^xsd:decimal ;
    :minutes "0"^^xsd:decimal ;
    :months  "1"^^xsd:decimal ;
    :seconds "0"^^xsd:decimal ;
    :weeks   "0"^^xsd:decimal ;
    :years   "0"^^xsd:decimal ;
.

:unitSecond
    rdf:type :TemporalUnit ;
    :days    "0"^^xsd:decimal ;
    :hours   "0"^^xsd:decimal ;
    :minutes "0"^^xsd:decimal ;
    :months  "0"^^xsd:decimal ;
    :seconds "1"^^xsd:decimal ;
    :weeks   "0"^^xsd:decimal ;
    :years   "0"^^xsd:decimal ;
.

:unitType
    rdf:type    owl:ObjectProperty ;
    rdfs:domain [ rdf:type    owl:Class ;
                  owl:unionOf ( :GeneralDateTimeDescription
                                :Duration ) ; ] ;
    rdfs:range  :TemporalUnit ;
.

:unitWeek
    rdf:type :TemporalUnit ;
    :days    "0"^^xsd:decimal ;
    :hours   "0"^^xsd:decimal ;
    :minutes "0"^^xsd:decimal ;
    :months  "0"^^xsd:decimal ;
    :seconds "0"^^xsd:decimal ;
    :weeks   "1"^^xsd:decimal ;
    :years   "0"^^xsd:decimal ;
.

:unitYear
    rdf:type :TemporalUnit ;
    :days    "0"^^xsd:decimal ;
    :hours   "0"^^xsd:decimal ;
    :minutes "0"^^xsd:decimal ;
    :months  "0"^^xsd:decimal ;
    :seconds "0"^^xsd:decimal ;
    :weeks   "0"^^xsd:decimal ;
    :years   "1"^^xsd:decimal ;
.

:week
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :GeneralDateTimeDescription ;
    rdfs:range  xsd:nonNegativeInteger ;
.

:weeks
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :GeneralDurationDescription ;
    rdfs:range  xsd:decimal ;
.

